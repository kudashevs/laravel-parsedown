{
    "name": "parsedown/laravel",
    "description": "Official Parsedown's Laravel Wrapper.",
    "keywords": [
        "parsedown",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "http://parsedown.org",
    "support": {
        "issues": "https://github.com/parsedown/laravel/issues",
        "source": "https://github.com/parsedown/laravel"
    },
    "authors": [
        {
            "name": "Eduardo Agostini",
            "email": "edu.agostini@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "orchestra/testbench": "^6.27|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Kudashevs\\LaravelParsedown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kudashevs\\LaravelParsedown\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kudashevs\\LaravelParsedown\\Providers\\ParsedownServiceProvider"
            ],
            "aliases": {
                "Parsedownn": "Kudashevs\\LaravelParsedown\\Facades\\ParsedownFacade"
            }
        }
    }
}
